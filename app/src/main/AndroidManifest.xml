<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ImoyaAndroidPreferenceLib">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SampleActivity"
            android:exported="false"
            android:label="@string/start_activity_sample" />

        <activity
            android:name=".SamplePlainActivity"
            android:exported="false"
            android:label="@string/start_plain_activity_sample" />

        <activity
            android:name=".SamplePlainActivityForFragment"
            android:exported="false"
            android:label="@string/start_plain_activity_and_fragment_sample" />

        <activity
            android:name="net.imoya.android.preference.activity.editor.list.MultiSelectionListEditorActivity"
            android:exported="false" />

        <activity
            android:name="net.imoya.android.preference.activity.editor.list.SingleSelectionListEditorActivity"
            android:exported="false" />

        <activity
            android:name="net.imoya.android.preference.activity.editor.time.TimeEditorActivity"
            android:exported="false" />

        <activity
            android:name="net.imoya.android.preference.activity.editor.time.TimePeriodEditorActivity"
            android:exported="false" />

    </application>

</manifest>